{
    "name": "AutoMart",
    "version": "1.0.0",
    "description": "An online marketplace for automobiles of diverse makes, model or body type",
    "main": "build/index.js",
    "scripts": {
        "dev": "nodemon --exec babel-node server/index.js",
        "test": "nyc mocha --require @babel/register server/test/**/**.spec.js --exit --timeout 500000",
        "build": "babel server --out-dir server/build",
        "prebuild": "rimraf server/build",
        "server": "babel-node ./server/",
        "start": "node server/build/index.js",
        "start:dev": "nodemon --exec babel-node server/index",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ChidiChuks/AutoMart.git"
    },
    "author": "Chidiebere Chukwuma",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ChidiChuks/AutoMart/issues"
    },
    "homepage": "https://github.com/ChidiChuks/AutoMart#readme",
    "dependencies": {
        "@babel/polyfill": "7.4.4",
        "bcrypt": "3.0.6",
        "cloudinary": "1.14.0",
        "debug": "4.1.1",
        "dotenv": "8.0.0",
        "express": "4.17.0",
        "jsonwebtoken": "8.5.1",
        "multer": "1.4.1",
        "npm-run-all": "4.1.5",
        "path": "0.12.7",
        "regenerator-runtime": "0.13.2",
        "winston": "3.2.1"
    },
    "devDependencies": {
        "@babel/cli": "7.4.4",
        "@babel/core": "7.4.4",
        "@babel/node": "7.2.2",
        "@babel/preset-env": "7.4.5",
        "@babel/register": "7.4.4",
        "babel-preset-env": "1.7.0",
        "braces": "3.0.2",
        "chai": "4.2.0",
        "chai-http": "4.3.0",
        "coveralls": "3.0.3",
        "eslint": "5.16.0",
        "eslint-config-airbnb-base": "13.1.0",
        "eslint-plugin-import": "2.17.2",
        "fs-minipass": "1.2.6",
        "istanbul": "0.4.5",
        "mocha": "6.1.4",
        "mocha-lcov-reporter": "1.3.0",
        "nodemon": "1.19.1",
        "nyc": "14.1.1",
        "rimraf": "2.6.3",
        "supertest": "4.0.2"
    },
    "nyc": {
        "exclude": [
            "**/test/**"
        ]
    },
    "engines": {
        "node": "11.14.0"
    },
    "plugins": [
        [
            "@babel/plugin-transform-regenerator",
            {
                "asyncGenerators": false,
                "generators": false,
                "async": false
            }
        ]
    ]
}